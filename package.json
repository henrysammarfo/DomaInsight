{
  "name": "domainsight-hackathon",
  "version": "1.0.0",
  "description": "AI-Driven Domain Scoring & Predictive Analytics dApp for Doma Protocol Hackathon Track 4",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "cd frontend && npm run build",
    "test": "cd backend && npm test && cd ../frontend && npm test",
    "test:api": "node test-api.js",
    "test:e2e": "node test-e2e.js",
    "test:live": "node tests/test-live.js",
    "deploy:backend": "chmod +x deploy-backend.sh && ./deploy-backend.sh",
    "deploy:frontend": "chmod +x deploy-frontend.sh && ./deploy-frontend.sh"
  },
  "keywords": [
    "doma",
    "domain",
    "ai",
    "scoring",
    "analytics",
    "defi",
    "blockchain",
    "hackathon",
    "trait-scoring"
  ],
  "author": "DomaInsight Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/DomaInsight-Hackathon.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/DomaInsight-Hackathon/issues"
  },
  "homepage": "https://github.com/yourusername/DomaInsight-Hackathon#readme",
  "devDependencies": {
    "axios": "^1.11.0",
    "concurrently": "^8.2.2",
    "jest": "^30.1.3",
    "puppeteer": "^24.20.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
